#presets.floating-menu
	.presets-inner
		.preset-filter
			.controlgroup
				label Category
				.controls
					.select-wrapper
						if currentArtboard.category
							select#category-selector(data-init-cate="#{currentArtboard.category.id}")
						else
							select#category-selector(data-init-cate="1")
							
			
			.controlgroup
				label Sub
				.controls
					.select-wrapper
						select#subcategory-selector

							
			.controlgroup.centered
				a.btn.confirmation.btn-update(href='javascript:;', data-action='update') Blank
		.preset-list
			each i in Array(20)
				.preset
					a(href='javascript:;')
						.thumbnail
							img(src='http://unsplash.it/400x300')

