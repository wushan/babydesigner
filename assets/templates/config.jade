#config(style='display: none;')
  .config-inner
    .panels
      .attributes-wrapper.panel
        .attributes-inner
          .block.attr.color
            .controlgroup
              label BGColor
              .controls
                input#objectColor(type='text')
          
          .block.attr.fontfamily.text-attr
            .controlgroup
              label Fonts
              .controls
                .select-wrapper
                  select#objectFontFamily
          .block.attr.color.text-attr
            .controlgroup
              label Color
              .controls
                input#objectTextColor(type='text')
          .block.attr.fontsize.text-attr
            .controlgroup
              label Font-size
              .controls
                .select-wrapper
                  select#objectFontSize
                    option(value='14') 14
                    option(value='18') 18
                    option(value='24') 24
                    option(value='30') 30
                    option(value='36') 36
                    option(value='48') 48
                    option(value='56') 56
                    option(value='64') 64
                    option(value='72') 72
                    option(value='144') 144